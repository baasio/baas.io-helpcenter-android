<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PullToRefresh">
        <attr format="reference|color" name="Helpcenter_ptrAdapterViewBackground" />
        <attr format="reference|color" name="Helpcenter_ptrHeaderBackground" />
        <attr format="color" name="Helpcenter_ptrHeaderTextColor" />
        <attr name="Helpcenter_ptrMode">
            <flag name="pullDownFromTop" value="0x1" />
            <flag name="pullUpFromBottom" value="0x2" />
            <flag name="both" value="0x3" />
        </attr>
    </declare-styleable>
</resources>

